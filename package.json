{
  "name": "astro-paper",
  "type": "module",
  "version": "5.5.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && pagefind --site dist && cp -r dist/_pagefind public/",
    "build:ci": "astro build && pagefind --site dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test:visual": "playwright test --config=playwright.config.ts",
    "test:visual:baseline": "playwright test --config=playwright.config.ts --update-snapshots",
    "test:visual:staging": "PLAYWRIGHT_TEST_BASE_URL=https://kyleskrinak.github.io/astro-blog playwright test --config=playwright.config.ts",
    "test:visual:production": "PLAYWRIGHT_TEST_BASE_URL=https://kyle.skrinak.com playwright test --config=playwright.config.ts",
    "test:visual:report": "playwright show-report",
    "test:console": "playwright test console-errors",
    "test:console:staging": "PLAYWRIGHT_TEST_BASE_URL=https://kyleskrinak.github.io/astro-blog playwright test console-errors",
    "test:console:production": "PLAYWRIGHT_TEST_BASE_URL=https://kyle.skrinak.com playwright test console-errors",
    "test:analytics": "playwright test --config=playwright.analytics.config.ts analytics-privacy.spec",
    "test:analytics:staging": "PLAYWRIGHT_TEST_BASE_URL=https://kyleskrinak.github.io/astro-blog playwright test --config=playwright.analytics.config.ts analytics-privacy.spec",
    "test:analytics:production": "PLAYWRIGHT_TEST_BASE_URL=https://kyle.skrinak.com playwright test --config=playwright.analytics.config.ts analytics-privacy.spec",
    "test:analytics:ga": "playwright test --config=playwright.analytics.config.ts analytics-privacy-ga",
    "test:analytics:ga:staging": "PLAYWRIGHT_TEST_BASE_URL=https://kyleskrinak.github.io/astro-blog playwright test --config=playwright.analytics.config.ts analytics-privacy-ga",
    "test:analytics:ga:production": "PLAYWRIGHT_TEST_BASE_URL=https://kyle.skrinak.com playwright test --config=playwright.analytics.config.ts analytics-privacy-ga",
    "test:seo": "playwright test --config=playwright.seo.config.ts tests/seo/seo-meta-tags.spec.ts",
    "test:seo:staging": "PLAYWRIGHT_TEST_BASE_URL=https://kyleskrinak.github.io/astro-blog playwright test --config=playwright.seo.config.ts tests/seo/seo-meta-tags.spec.ts",
    "test:seo:production": "PLAYWRIGHT_TEST_BASE_URL=https://kyle.skrinak.com playwright test --config=playwright.seo.config.ts tests/seo/seo-meta-tags.spec.ts",
    "test:sitemap": "playwright test --config=playwright.seo.config.ts tests/seo/sitemap.spec.ts",
    "test:sitemap:staging": "PLAYWRIGHT_TEST_BASE_URL=https://kyleskrinak.github.io/astro-blog playwright test --config=playwright.seo.config.ts tests/seo/sitemap.spec.ts",
    "test:sitemap:production": "PLAYWRIGHT_TEST_BASE_URL=https://kyle.skrinak.com playwright test --config=playwright.seo.config.ts tests/seo/sitemap.spec.ts",
    "test:links": "htmltest",
    "test:links:verbose": "htmltest -l 3",
    "check:links": "node scripts/check-links.js"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "dayjs": "^1.11.19",
    "lodash.kebabcase": "^4.1.1",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.18.3",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@pagefind/default-ui": "^1.4.0",
    "@playwright/test": "^1.57.0",
    "@shikijs/transformers": "^3.20.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/lodash.kebabcase": "^4.1.9",
    "@typescript-eslint/parser": "^8.51.0",
    "broken-link-checker": "^0.7.8",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^16.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-table-of-contents": "^1.1.0",
    "pagefind": "^1.4.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}
